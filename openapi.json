        "type" : "object",
        "properties" : {
          "projectName" : {
            "maxLength" : 50,
            "minLength" : 3,
            "type" : "string",
            "example" : "My Project"
          }
        }
      },
      "ProjectLatestItems" : {
        "required" : [ "id", "projectName" ],
